upE: ['C-01','C-02','C-03']\r\n      },\r\n      timer: \"\"\r\n    }\r\n  },\r\n  computed: {\r\n    panelParts () {\r\n      return this.$store.state.panelParts\r\n    }\r\n  },\r\n  methods: {\r\n    getPanelInfo(pid) {\r\n      return this.$store.getters.getPanelById(pid)\r\n    },\r\n    fixNum(num) {\r\n      return (''+num).length < 2 ? ((new Array(2 + 1)).join('0') + num).slice(-2) : '' + num;\r\n    },\r\n    showMap(id) {\r\n      clearInterval(this.timer);\r\n      this.activedBlock = id;\r\n      this.toggleTab = 'map';\r\n      this.clearBlock();\r\n    },\r\n    clearBlock() {\r\n      this.timer = setInterval(() => {\r\n        this.activedBlock = \"\";\r\n        clearInterval(this.timer);\r\n      }, 6000)\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}